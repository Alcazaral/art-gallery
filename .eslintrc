{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "plugins": [
    "babel"
  ],
  "rules": {
    "global-require": 0,
    "no-param-reassign": 0,
    "arrow-body-style": 0,
    "consistent-return": 0,
    "prefer-arrow-callback": 0,
    "max-len": ["warn", 80],
    "quotes": 0,
    "no-return-assign": 0,
    "no-underscore-dangle": 0,
    "no-alert": 0,
    "no-nested-ternary": 0,
    "no-plusplus": 0,
    "no-continue": 0,
    "array-callback-return": 0,

    "react/prop-types": 0,
    "jsx-a11y/no-static-element-interactions": 0,
    "react/no-array-index-key": 0,

    "require-yield": 0,
    "generator-star-spacing": 0,
    "babel/generator-star-spacing": 1,

    "import/no-unresolved": 0,
    "import/extensions": 0
  },
  "env": {
    "browser": true,
    "node": true
  },
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [".js", ".jsx", ".json", ".scss"]
      }
    }
  }
}
